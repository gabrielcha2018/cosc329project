    "import pandas as pd\n",
    "import glob\n",
    "\n",
    "all_data = pd.DataFrame()\n",
    "allfiles = glob.glob( \"process/*.csv\" )\n",
    "\n",
    "for f in allfiles:\n",
    "    df = pd.read_csv(f)\n",
    "    all_data = all_data.append(df,ignore_index=True)\n",
    "\n",
    "\n",
    "\n",
    "all_data.to_csv(\"new_combined_file.csv\") \n",
    "\n",
    "print('done')\n",
    "\n",
    "all_data"
   ]
